var Segment s;
var Point p;
var Paire m;

class Point {
  attribute int x;
  attribute int y;
}

class Segment {
  attribute Point a;
  attribute Point b;
}

class Paire {
  attribute int x;
  attribute int y;

  method void constructor(int x, int y) {
    this.x = x;
    this.y = y;
  }

  method int somme() {
    return this.x + this.y;
  }
}

main {
  p = new Point;
  p.x = 1;
  p.y = 2;
  s = new Segment;
  s.a = p;
  s.b = new Point;
  s.b.x = 3;
  p = s.b;
  p.y = 4;
  print(s.a.x + s.a.y + s.b.x + s.b.y); // 10
  m = new Paire(1, 2);
  print(m.x - m.y); // -1
  print(---3);
}
