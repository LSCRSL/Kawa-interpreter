var segment s;
var point p;
var paire m;

class point {
  attribute int x;
  attribute int y;
}

class segment {
  attribute point a;
  attribute point b;
}

class paire {
  attribute int x;
  attribute int y;

  method void constructor(int x, int y) {
    this.x = x;
    this.y = y;
  }

  method int somme() {
    return this.x + this.y;
  }
}

main {
  p = new point;
  p.x = 1;
  p.y = 2;
  s = new segment;
  s.a = p;
  s.b = new point;
  s.b.x = 3;
  p = s.b;
  p.y = 4;
  print(s.a.x + s.a.y + s.b.x + s.b.y);
  // affiche 10
  m = new paire(1, 2);
  print(m.x - m.y);
  print(---3);
}
