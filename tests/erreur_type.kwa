var point p;
var bool x;

class point {
    attribute int x;
    attribute int y;

    method int constructor(int x, int y) {
        this.x = x;
        this.y = y;
    } 

    method int test (int px) {
        var int l;
        l = px;
        return l+1;
    }
}

main {
    p = new point(1, 2);
    x = p.test(1);
}